.body01{ background:#f0efea; color:#5a574e;}
a {color: #5f574a;text-decoration: none;}
.fixed{ position:fixed; top:0;}
.fixed{_position:absolute; _top:expression((20+(noValue = document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop))+'px');}
.header{ height:140px; background:#72c5b3 url(../images/topbg_03.gif) left bottom repeat-x;}
